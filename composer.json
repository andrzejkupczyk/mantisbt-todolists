{
    "name": "webgarden/mantisbt-todolists",
    "description": "To-Do Lists Plugin for MantisBT",
    "type": "mantisbt-plugin",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Andrzej Kupczyk",
            "email": "kontakt@andrzejkupczyk.pl"
        }
    ],
    "support": {
        "issues": "https://github.com/andrzejkupczyk/mantisbt-todolists/issues"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mantisbt/mantisbt",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/mantisbt/securimage",
            "no-api": true
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "ext-json": "*",
        "composer/installers": "~1.0",
        "webgarden/mantisbt-termite": "^1.0"
    },
    "require-dev": {
        "mantisbt/mantisbt": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "WebGarden\\ToDoLists\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.0.0"
        },
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "installer-name": "ToDoLists",
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "minimum-stability": "dev"
}
